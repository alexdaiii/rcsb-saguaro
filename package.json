{
  "name": "@rcsb/rcsb-saguaro",
  "version": "2.0.3",
  "description": "RCSB 1D Feature Viewer",
  "main": "build/RcsbFv.js",
  "files": [
    "build/RcsbFv.js",
    "build/*ts",
    "build/**/*ts",
    "build/**/**/*ts",
    "build/**/**/**/*ts",
    "build/rcsb-saguaro.js"
  ],
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "buildApp": "npm run build",
    "buildAll": "npm run buildApp && npm run buildAllDoc",
    "buildDoc": "npx typedoc ./src/RcsbFv.ts && sed -i '' '/---/d' docs/index.html",
    "cpExamples": "ncp examples docs/examples",
    "buildAllDoc": "npm run buildDoc && npm run cpExamples",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rcsb/rcsb-saguaro.git"
  },
  "keywords": [
    "1D",
    "sequence",
    "annotations",
    "gene",
    "protein"
  ],
  "author": "Joan Segura Mora <joan.segura@rcsb.org>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "ncp": "^2.0.0",
    "react-icons": "^4.3.1",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^6.2.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@d3fc/d3fc-sample": "^4.1.0",
    "@popperjs/core": "^2.4.4",
    "@types/d3": "^5.7.2",
    "d3": "^5.16.0",
    "d3-shape": "^1.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "rxjs": "^6.6.0"
  },
  "bugs": {
    "url": "https://github.com/rcsb/rcsb-saguaro/issues"
  },
  "homepage": "https://rcsb.github.io/rcsb-saguaro",
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}
